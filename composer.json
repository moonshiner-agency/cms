{
    "name": "moonshiner/cms",
    "description": "Finally a good CMS for the Laravel Framework.",
    "keywords": ["cms", "laravel"],
    "license": "MIT",
    "homepage": "https://cms.com",
    "support": {
        "issues": "https://github.com/moonshiner/cms/issues",
        "source": "https://github.com/moonshiner/cms"
    },
    "authors": [
        {
            "name": "Florian Bauer",
            "email": "florian@moonshiner.at"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "~5.4",
        "illuminate/http": "~5.4"
    },
    "require-dev": {
        "mockery/mockery": "~0.9",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Moonshiner\\Cms\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
